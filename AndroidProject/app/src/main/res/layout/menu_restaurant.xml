<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Menu_restaurant">

    <TextView
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="155dp"
        android:background="#0C196E"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/horaire_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="140dp"
        android:layout_marginTop="65dp"
        android:text="Menu"
        android:textColor="@color/white"
        android:textSize="50dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="228dp"
        android:layout_height="50dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="23dp"
        android:text="Desserts"
        android:textColor="#070707"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="228dp"
        android:layout_height="50dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="29dp"
        android:text="Entrées"
        android:textColor="#070707"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="228dp"
        android:layout_height="50dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="35dp"
        android:text="Plats Principaux"
        android:textColor="#070707"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar" />


    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="11dp"
        android:layout_height="14dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="27dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView16"
        app:srcCompat="@drawable/ic_point" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="11dp"
        android:layout_height="14dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5"
        app:srcCompat="@drawable/ic_point" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="11dp"
        android:layout_height="14dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="27dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        app:srcCompat="@drawable/ic_point" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="11dp"
        android:layout_height="14dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4"
        app:srcCompat="@drawable/ic_point" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="11dp"
        android:layout_height="14dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:srcCompat="@drawable/ic_point" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="11dp"
        android:layout_height="14dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView6"
        app:srcCompat="@drawable/ic_point" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="11dp"
        android:layout_height="14dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="26dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView15"
        app:srcCompat="@drawable/ic_point" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="207dp"
        android:layout_height="26dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="23dp"
        android:text="Boeuf bourguignon"
        android:textColor="#050505"
        app:layout_constraintStart_toEndOf="@+id/imageView6"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="207dp"
        android:layout_height="26dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="24dp"
        android:text="Cuisses de grenouille"
        android:textColor="#050505"
        app:layout_constraintStart_toEndOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="207dp"
        android:layout_height="26dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="1dp"
        android:text="Escargots rôtis"
        android:textColor="#050505"
        app:layout_constraintStart_toEndOf="@+id/imageView8"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="207dp"
        android:layout_height="26dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="24dp"
        android:text="Île flottante"
        android:textColor="#040404"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/textView16" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="207dp"
        android:layout_height="26dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="1dp"
        android:text="Crème brûlée"
        android:textColor="#040404"
        app:layout_constraintStart_toEndOf="@+id/imageView7"
        app:layout_constraintTop_toBottomOf="@+id/textView10" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="207dp"
        android:layout_height="26dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="1dp"
        android:text="Blanquette de veau"
        android:textColor="#050505"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="207dp"
        android:layout_height="26dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="1dp"
        android:text="Quiche lorraine"
        android:textColor="#050505"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

</androidx.constraintlayout.widget.ConstraintLayout>